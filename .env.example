# Blink Market Environment Variables
# Copy this file to .env.local and fill in your values

# =============================================================================
# ZKLOGIN CONFIGURATION (Required for Google Login)
# =============================================================================

# Google OAuth Client ID
# Get this from Google Cloud Console: https://console.cloud.google.com/apis/credentials
# 1. Create a new OAuth 2.0 Client ID
# 2. Set authorized redirect URI to your app's origin (e.g., http://localhost:5173)
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Enoki public API key (safe for frontend)
VITE_ENOKI_PUBLIC_API_KEY=enoki_public_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Redirect URI after Google login (defaults to window.location.origin)
# VITE_ZKLOGIN_REDIRECT_URI=http://localhost:5173

# Salt Service URL (optional)
# For custom zkLogin backend integrations:
# VITE_SALT_SERVICE_URL=/api/salt
# Or for production:
# VITE_SALT_SERVICE_URL=https://your-backend.example.com/get_salt

# Proving Service URL (optional)
# For custom zkLogin backend integrations:
# VITE_PROVING_SERVICE_URL=/api/prover
# Or for production:
# VITE_PROVING_SERVICE_URL=https://your-backend.example.com/v1

# =============================================================================
# LI.FI CONFIGURATION (Optional)
# =============================================================================

# LI.FI API Key (optional - for higher rate limits)
# VITE_LIFI_API_KEY=your-lifi-api-key

# =============================================================================
# SUI NETWORK CONFIGURATION
# =============================================================================

# Default network: mainnet, testnet, or devnet
VITE_SUI_NETWORK=testnet

# =============================================================================
# BLINK CONTRACT CONFIGURATION (Required for on-chain place bet)
# =============================================================================

# Published package ID for Blink contract
# VITE_BLINK_PACKAGE_ID=0x...

# Shared object IDs
# VITE_BLINK_MARKET_ID=0x...
# VITE_BLINK_TREASURY_ID=0x...
# VITE_BLINK_EVENT_ID=0x...
